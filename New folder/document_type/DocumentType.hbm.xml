<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.tdms.model.document_type">

	<class name="DocumentType" table="Document_Type" schema="tdms">
		<id name="id" type="long">
			<generator class="increment" />
		</id>
		<many-to-one name="folderType" 
			class="ir.viratech.tdms.model.folder_type.FolderType" 
			column="folderTypeId" not-null="true"
			cascade="none" />
		
		<property name="extuid" type="string" not-null="true" unique="true" >
			<meta attribute="alternate-key">true</meta>
		</property>
		<property name="title" type="string" not-null="true" />
		<property name="description" type="string" />
		<property name="deleted" type="boolean" not-null="true"/>	
		
	</class>

</hibernate-mapping>