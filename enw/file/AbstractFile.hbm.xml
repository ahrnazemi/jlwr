<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.tdms.model.file">

	<class name="AbstractFile" table="ABSTRACT_FILES" schema="tdms">
		<id name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<many-to-one name="fileType"
			class="ir.viratech.tdms.model.file_type.FileType" unique="false"
			insert="true" update="true" column="fileTypeId" not-null="true" />

		<many-to-one name="document"
			class="ir.viratech.tdms.model.document.Document" unique="false"
			insert="true" update="true" column="documentId" not-null="true" />

		<property name="hashCodeString" column="hashCodeStr" type="string"
			unique="true" not-null="true" />
	</class>
</hibernate-mapping>
