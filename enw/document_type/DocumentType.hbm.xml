<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.tdms.model.document_type">

	<class name="DocumentType" table="Document_Type" schema="tdms">
		<id name="id" type="long">
			<generator class="increment" />
		</id>


		<set name="allDTFTA" inverse="true" cascade="all-delete-orphan">
			<key column="documentTypeId" not-null="false" />
			<one-to-many class="ir.viratech.tdms.model.file_type.DTFTA" />
		</set>

		<many-to-one name="ftdta"
			class="ir.viratech.tdms.model.document_type.FTDTA" cascade="all"
			not-null="false" unique="true" column="ftdtaId" />

		<set name="allDocuments" cascade="all-delete-orphan" inverse="true">
			<key column="documentTypeId" not-null="true" />
			<one-to-many class="ir.viratech.tdms.model.document.Document" />
		</set>

		<property name="extuid" type="string" not-null="true" unique="true">
			<meta attribute="alternate-key">true</meta>
		</property>
		<property name="title" type="string" not-null="true" />
		<property name="description" type="string" />
		<property name="deleted" type="boolean" not-null="true" />

	</class>

</hibernate-mapping>