<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.tdms.model.file_type">

	<class name="FileType" table="File_Type" schema="tdms">
		<id name="id" type="long">
			<generator class="increment" />
		</id>

		<many-to-one name="dtfta" class="ir.viratech.tdms.model.file_type.DTFTA"
			cascade="all" not-null="false" unique="true" column="dtftaId" />

		<set name="allFile" inverse="true" cascade="all-delete-orphan">
			<key column="fileTypeId" not-null="false" />
			<one-to-many class="ir.viratech.tdms.model.file.AbstractFile" />
		</set>

		<property name="extuid" type="string" not-null="true" unique="true">
			<meta attribute="alternate-key">true</meta>
		</property>
		<property name="title" type="string" not-null="true" />
		<property name="maxSize" type="int" />
		<property name="types" type="string" />
		<property name="deleted" type="boolean" not-null="true" />

	</class>

</hibernate-mapping>