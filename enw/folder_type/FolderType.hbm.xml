<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.tdms.model.folder_type">
	<class name="FolderType" table="Folder_Type" schema="tdms">
		<id name="id" type="long">
			<generator class="increment" />
		</id>


		<property name="extuid" type="string" not-null="true" unique="true">
			<meta attribute="alternate-key">true</meta>
		</property>
		<property name="title" type="string" not-null="true" />


		<set name="allFTDTA" inverse="true" cascade="all-delete-orphan">
			<key column="folderTypeId" not-null="false" />
			<one-to-many class="ir.viratech.tdms.model.document_type.FTDTA" />
		</set>

		<set name="allFolder" inverse="true" cascade="all-delete-orphan">
			<key column="folderTypeId" not-null="false" />
			<one-to-many class="ir.viratech.tdms.model.folder.Folder" />
		</set>

		<property name="description" type="string" />
		<property name="deleted" type="boolean" not-null="true" />
	</class>

</hibernate-mapping>